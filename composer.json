{
    "name": "drupal-utils/entity-convert",
    "description": "Converts Drupal entity objects into simple array of fields.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vishwa Chikate",
            "email": "vishwa.chikate@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "DrupalUtils\\EntityConvert\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DrupalUtils\\EntityConvertTest\\": "test/"
        }
    },
    "require": {
        "php": "^7.2 || ^8.0"
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "9.5.x-dev",
        "phpstan/phpstan": "^1.8"
    },
    "scripts": {
        "tests": "./vendor/bin/phpunit --colors test/",
        "phpstan": "./vendor/bin/phpstan analyse src test"
    }
}
